<Window x:Class="JWAPICaseAttributes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="10,10,10,10">
		<Grid.RowDefinitions>
			<RowDefinition Height="50"/>
			<RowDefinition Height="25"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition/>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<StackPanel Grid.Row="0" Grid.Column="0">
			<TextBlock Text="CaseID"/>
			<TextBox Text="{Binding QueryCaseID, Mode=TwoWay}" />
		</StackPanel>
		<Button Grid.Column="1" Grid.Row="0" Margin="30,10,30,15" Content="Search" Click="Button_Click_1"/>
		<TextBlock Grid.Row="1" Grid.ColumnSpan="3" Text="{Binding Status}"/>
	</Grid>
</Window>
